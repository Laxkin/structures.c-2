//c structures
#include <stdio.h>
#include <string.h>
struct student{
    char name[30];
    float height;
    char reg_no[20];
    char email[30];
    int phone;
}student1, student2;
int main() {
    // struct student1,student2;
   
   printf("Enter your name:");
   scanf("%s",&student1.name);
   printf("Enter your height:");
   scanf("%f",&student1.height);
   printf("Enter your registration number:");
   scanf("%f",&student1.reg_no);
   printf("Enter your email:");
   scanf("%s",&student1.email);
   printf("Enter your phone number:");
   scanf("%d",&student1.phone);
  
  
    printf("Name:%s\n",student1.name);
    printf("Height:%.1f\n",student1.height);
    printf("Reg_no:%s\n",student1.reg_no);
    printf("Email:%s\n",student1.email);
    printf("Phone number:%d\n",student1.phone);
    
    return 0;
}
